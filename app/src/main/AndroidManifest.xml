<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tk.parmclee.o_droid">

    <uses-feature android:name="android.hardware.location.gps"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/android012"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapListActivity"/>
        <activity android:name=".Preferences"/>
        <activity android:name=".LevelActivity"/>
        <activity android:name=".AffixmentActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".ExploreActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".CreateRouteActivity"
            android:screenOrientation="nosensor"/>
        <activity android:name=".CompetitionActivity"
            android:screenOrientation="nosensor"/>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="e9bca97b3a4e6c0afe0b8138a0edbac2e94e9683" />
    </application>

</manifest>